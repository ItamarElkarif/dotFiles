    "csharp.suppressDotnetInstallWarning": true,
    "editor.wordWrap": "on",
    "editor.wordWrapColumn": 120,
    "cSpell.userWords": [
        "Itamar",
        "MULT",
        "argparse",
        "cuze",
        "itertools",
        "pytest",
        "respawn"
    ],
    "editor.multiCursorModifier": "alt",
    "javascript.updateImportsOnFileMove.enabled": "always",
    "bracketPairColorizer.highlightActiveScope": true,
    "bracketPairColorizer.showBracketsInRuler": true,
    "terminal.integrated.fontSize": 16,
    "editor.fontSize": 15,
    "vim.easymotionMarkerFontSize": "16",
    "workbench.colorTheme": "Cobalt2",
    "window.zoomLevel": 0,
    "omnisharp.disableMSBuildDiagnosticWarning": true,
    "todo-tree.highlights.enabled": true,
    "java.errors.incompleteClasspath.severity": "ignore",
    "editor.codeActionsOnSave": {
        "source.fixAll.eslint": true
    },
    "vim.easymotion": true,
    "vim.leader": " ",
    "vim.easymotionMarkerBackgroundColor": "#5032a8",
    "vim.easymotionMarkerHeight": 16,
    "vim.easymotionMarkerWidthPerChar": 12,
    "vim.statusBarColorControl": true,
    "vim.replaceWithRegister": true,
    "vim.surround": true,
    "vim.timeout": 0,
    "vim.gdefault": true,
    "vim.highlightedyank.enable": true,
    "vim.vimrc.enable": true,
    "workbench.startupEditor": "newUntitledFile",
    "[markdown]": {
        "editor.defaultFormatter": "mervin.markdown-formatter"
    },
    "python.linting.pylintEnabled": false,
    "python.linting.flake8Enabled": true,
    "python.linting.flake8Args": [
        "--config=./game/lib/setup.cfg"
    ],
    "eslint.options": {
        "configFile": "./game/lib/visualizer/config/.eslintrc",
        "ignoreFile": "./game/lib/visualizer/config/.eslintignore"
    },
    "json.schemas": [
        {
            "fileMatch": [
                "/game/lib/plugins/**/manifest.json"
            ],
            "url": "/tools/schemas/manifest.schema.json"
        }
    ],
    "path-intellisense.mappings": {
        "~": "${workspaceRoot}/game/lib",
        "~plugins": "${workspaceRoot}/game/lib/plugins",
        "~root": "${workspaceRoot}/game/lib/plugins/root/visualizer/js",
        "~src": "${workspaceRoot}/game/lib/visualizer/src",
        "~PluginManager": "${workspaceRoot}/game/lib/visualizer/src/bin/PluginManager.json"
    },
    "todo-tree.tree.showScanModeButton": false
}


