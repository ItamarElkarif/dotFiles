{
    "workbench.iconTheme": "vscode-icons",
    "editor.suggestSelection": "first",
    "editor.fontLigatures": true,
    "editor.fontFamily": "'Fira Code', 'monospace', 'Droid Sans Mono', 'Droid Sans Fallback'",
    "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
    "python.jediEnabled": false,
    "files.autoSave": "afterDelay",
    "editor.rulers": [
        120
    ],
    "workbench.colorCustomizations": {
        "[Cobalt2]": {
            "merge.border": "#ca6f3a",
            "merge.commonContentBackground": "#ff0000",
            "merge.commonHeaderBackground": "#a3ad42",
            "merge.currentContentBackground": "#1ab6688e",
            "merge.currentHeaderBackground": "#1ebe66bd",
            "merge.incomingContentBackground": "#289fbd9c",
            "merge.incomingHeaderBackground": "#03fdf07e"
        },
        "statusBar.background": "#5f00af",
        "statusBar.noFolderBackground": "#5f00af",
        "statusBar.debuggingBackground": "#5f00af"
    },
    "editor.tokenColorCustomizations": {
        "textMateRules": [
            {
                "scope": "keyword",
                "settings": {
                    "foreground": "#cf8b25"
                }
            }
        ]
    },
    "files.exclude": {
        "**/.git": true,
        "**/.svn": true,
        "**/.hg": true,
        "**/CVS": true,
        "**/.DS_Store": true,
        "**/*.pyc": true,
        "**/__pycache__": true,
        "**/.classpath": true,
        "**/.project": true,
        "**/.settings": true,
        "**/.factorypath": true
    },
    "cSpell.enableFileTypes": [
        "asciidoc",
        "c",
        "cpp",
        "csharp",
        "css",
        "git-commit",
        "go",
        "handlebars",
        "haxe",
        "html",
        "jade",
        "java",
        "javascript",
        "javascriptreact",
        "json",
        "jsonc",
        "latex",
        "less",
        "markdown",
        "php",
        "plaintext",
        "pug",
        "python",
        "restructuredtext",
        "rust",
        "scala",
        "scss",
        "text",
        "typescript",
        "typescriptreact",
        "yaml",
        "yml"
    ],
    "css.lint.argumentsInColorFunction": "warning",
    "editor.formatOnPaste": true,
    "eslint.validate": [
        "javascript",
        "javascriptreact",
        "html",
        "python",
        "haxe",
        "csharp",
        "cpp"
    ],
    "vim.overrideCopy": true,
    "vim.useCtrlKeys": true,
    "vim.hlsearch": true,
    "vim.incsearch": true,
    "vim.ignorecase": true,
    "vim.sneakUseIgnorecaseAndSmartcase": true,
    "vim.handleKeys": {
        "<C-k>": false,
        "<C-f>": false
    },
